# AWS Configuration
AWS_REGION=ca-central-1
AWS_ACCOUNT_ID=your-account-id

# Environment
ENVIRONMENT=development

# API Configuration
API_URL=https://api.yourdomain.com/v1

# Cognito
COGNITO_USER_POOL_ID=
COGNITO_CLIENT_ID=
COGNITO_IDENTITY_POOL_ID=

# S3
S3_BUCKET_NAME=idv-documents-{account-id}-ca-central-1

# KMS
KMS_KEY_ID=

# Feature Flags
ENABLE_LIVENESS_CHECK=true
ENABLE_MANUAL_REVIEW=true

# Thresholds (can be overridden)
SIMILARITY_PASS_THRESHOLD=90
SIMILARITY_FAIL_THRESHOLD=70
OCR_PASS_THRESHOLD=85
OCR_FAIL_THRESHOLD=70

# Limits
MAX_VERIFICATION_ATTEMPTS=3
IMAGE_RETENTION_DAYS=30
REVIEW_SLA_HOURS=24
